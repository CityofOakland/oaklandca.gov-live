<hr style="border-top: 2px solid #212121;opacity:1;"/>
<h4>{{ block.heading ? block.heading : 'Related Documents' }}</h4>
<ul class="related">
	{% for entry in block.documents.all() %}
		<li><a href="{{ entry.url }}">{{ svg( '@webroot/assets/img/icon-pdf-updated.svg') }} {{ entry.title }}</a></li>
	{% endfor %}
</ul>