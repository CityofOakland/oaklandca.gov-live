{% if entry.timeline.exists() %}
  <section class="mb-16">
    <a class="anchor-link" id="page-timeline"></a>
    {% include '_partials/_elements/_module-header.twig' with {
      headerText: headerText ?? "Timeline"
    } %}
    <dl class="timeline-grid">
      {% for block in entry.timeline.all() %}
        <dt>{{ block.milestoneName }}</dt>
        <dd>{{ block.milestoneDates }}</dd>
      {% endfor %}
    </dl>
  </section>
{% endif %}
