{% if currentUser %}<!-- Begin {{ _self }} -->{% endif %}
<div class="relative z-0 pb-12">
	<div class="components">
		{% set url = parseUrl(block.embed) %}
		{% set params = url.query|split("&") %}
		{% set idParam = params[0] %}
		{% set id = idParam|split("=")[1] %}
		<iframe
			class="h-auto w-full aspect-[16/9]"
			width="560"
			height="315"
			src="https://www.youtube.com/embed/{{ id }}"
			title="YouTube video player"
			allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
			allowfullscreen>
		</iframe>
	</div>
</div>
{% if currentUser %}<!-- End {{ _self }} -->{% endif %}
