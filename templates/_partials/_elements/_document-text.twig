{% if currentUser %}<!-- Begin {{ _self }} -->{% endif %}
<a href="{{ file.url }}" class="flex items-center duration-75 fill-current">
	<div class="inline-block w-8 h-5">
    {{ svg('@webroot/dist/img/icon-' ~ file.kind ~ '.svg')|attr({
      class: 'w-full h-full'
    })|raw }}
  </div>
	{{ entry.title }}
	({{ file.kind | upper }})
</a>
<span class="block pl-8 mt-0 text-sm">
	<a href="{{ entry.url }}">Permalink</a>
</span>
