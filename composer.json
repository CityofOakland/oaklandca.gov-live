    "mmikkel/retcon": "2.7.5",
    "nystudio107/craft-emptycoalesce": "4.0.0",
    "nystudio107/craft-retour": "4.1.15",
    "nystudio107/craft-seomatic": "4.0.40",
    "nystudio107/craft-twigpack": "4.0.0-beta.4",
    "nystudio107/craft-typogrify": "4.0.1",
    "nystudio107/craft-vite": "4.0.8",
    "percipioglobal/craft-colour-swatches": "4.3.0",
    "presseddigital/linkit": "4.0.4.1",
    "putyourlightson/craft-blitz": "4.11.2",
    "putyourlightson/craft-sherlock": "4.4.1",
    "rynpsc/craft-phone-number": "2.2.0",
    "sentry/sdk": "^3.1",
    "solspace/craft-calendar": "4.0.11",
    "solspace/craft-freeform": "4.1.13",
    "spacecatninja/imager-x": "4.3.1",
    "spicyweb/craft-neo": "4.0.5",
    "statikbe/craft-sentry": "2.0.1",
    "studioespresso/craft-dumper": "5.0.0",
    "studioespresso/craft-scout": "3.3.2",
    "superbig/craft3-beam": "3.0.0",
    "superbig/craft3-templateselect": "3.0.0",
    "topshelfcraft/wordsmith": "4.4.0",
    "unionco/calendarize": "2.0.0",
    "vaersaagod/matrixmate": "2.1.4",
    "verbb/buttonbox": "4.2.0",
    "verbb/icon-picker": "2.0.16",
    "verbb/patrol": "5.0.2",
    "verbb/redactor-tweaks": "3.0.1",
    "verbb/smith": "2.0.0",
    "verbb/super-table": "3.0.12",
    "verbb/tablemaker": "4.0.7",
    "vlucas/phpdotenv": "^5.4.0",
    "wrav/related": "2.1.1",
    "verbb/default-dashboard": "2.0.1",
    "ether/logs": "4.0.0",
    "trendyminds/craft-reporter": "2.2.0",
    "panlatent/schedule": "0.4.0.1",
    "ostark/craft-async-queue": "3.1.1",
    "putyourlightson/craft-dashboard-begone": "2.0.0",
    "towardstudio/asset-locations": "2.3.0",
    "nystudio107/craft-twigprofiler": "4.0.0-beta.1"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "modules\\oaklandlibraryeventsmodule\\": "modules/oaklandlibraryeventsmodule/src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true,
      "php-http/discovery": true,
      "treeware/plant": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ],
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "require-dev": {
    "nystudio107/craft-autocomplete": "^1.11"
  }
}