{
  "name": "cityofoakland/oaklandca.gov",
  "description": "The official Craft CMS website for the City of OaklandCA.gov",
  "require": {
    "craftcms/cms": "4.8.9",
    "besteadfast/craft-preparse-field": "2.0.2",
    "craftcms/aws-s3": "2.2.1",
    "craftcms/feed-me": "5.4.0",
    "craftcms/store-hours": "3.1.0",
    "doublesecretagency/craft-googlemaps": "4.5.2",
    "doublesecretagency/craft-inventory": "3.2.0",
    "doublesecretagency/craft-viewcount": "1.2.3",
    "hybridinteractive/craft-position-fieldtype": "4.0.0",
    "marionnewlevant/twig-perversion": "3.0.0",
    "mmikkel/incognito-field": "1.3.0",
    "mmikkel/retcon": "2.7.5",
    "nystudio107/craft-emptycoalesce": "4.0.0",
    "nystudio107/craft-retour": "4.1.16",
    "nystudio107/craft-seomatic": "4.0.48",
    "nystudio107/craft-typogrify": "4.0.3",
    "nystudio107/craft-vite": "4.0.9",
    "presseddigital/linkit": "4.0.4.1",
    "putyourlightson/craft-blitz": "4.16.2",
    "putyourlightson/craft-sherlock": "4.4.2",
    "rynpsc/craft-phone-number": "2.2.0",
    "sentry/sdk": "^3.1",
    "solspace/craft-calendar": "5.0.3",
    "solspace/craft-freeform": "5.1.13",
    "spacecatninja/imager-x": "4.4.0",
    "spicyweb/craft-neo": "4.1.2",
    "studioespresso/craft-dumper": "5.0.1",
    "superbig/craft3-beam": "3.0.0",
    "superbig/craft3-templateselect": "3.0.0",
    "topshelfcraft/wordsmith": "4.4.0",
    "unionco/calendarize": "2.0.0",
    "vaersaagod/matrixmate": "2.1.4",
    "verbb/buttonbox": "4.2.4",
    "verbb/icon-picker": "2.0.17",
    "verbb/patrol": "5.0.2",
    "verbb/smith": "2.0.1",
    "verbb/super-table": "3.0.13",
    "verbb/tablemaker": "4.0.10",
    "vlucas/phpdotenv": "^5.4.0",
    "putyourlightson/craft-dashboard-begone": "2.0.0",
    "nystudio107/craft-twigprofiler": "4.0.0-beta.1",
    "studioespresso/craft-scout": "4.0.0",
    "craftpulse/craft-colour-swatches": "4.4.0",
    "roberskine/craft-user-manual": "5.0.0",
    "craftcms/ckeditor": "3.8.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "modules\\oaklandlibraryeventsmodule\\": "modules/oaklandlibraryeventsmodule/src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true,
      "php-http/discovery": true,
      "treeware/plant": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ],
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "require-dev": {
    "nystudio107/craft-autocomplete": "^1.11"
  }
}
