{
  "name": "cityofoakland/oaklandca.gov",
  "description": "The official Craft CMS website for the City of OaklandCA.gov",
  "require": {
    "craftcms/cms": "3.8.5",
    "vlucas/phpdotenv": "^2.4.0",
    "nystudio107/craft-typogrify": "1.1.20",
    "nystudio107/craft-seomatic": "3.4.36",
    "craftcms/store-hours": "2.2.0",
    "craftcms/redactor": "2.10.12",
    "verbb/redactor-tweaks": "2.0.4",
    "aelvan/inlin": "2.1.1",
    "doublesecretagency/craft-cpcss": "2.4.0",
    "doublesecretagency/craft-cpbodyclasses": "2.2.1",
    "marionnewlevant/twig-perversion": "2.2.0",
    "mikestecker/craft-videoembedder": "1.1.4",
    "rynpsc/craft-phone-number": "1.5.0",
    "nystudio107/craft-retour": "3.2.9",
    "nystudio107/craft-emptycoalesce": "1.0.6",
    "craftcms/aws-s3": "1.3.2",
    "superbig/craft3-beam": "2.1.5",
    "wrav/related": "1.2.0",
    "nystudio107/craft-twigpack": "1.2.17",
    "craftcms/feed-me": "4.6.1",
    "mmikkel/incognito-field": "1.3.0",
    "born05/craft-assetusage": "2.3.1",
    "verbb/super-table": "2.7.4",
    "verbb/smith": "1.2.3",
    "vaersaagod/matrixmate": "1.5.0",
    "craftcms/element-api": "2.8.6.1",
    "mmikkel/retcon": "2.6.1",
    "superbig/craft3-templateselect": "2.0.2",
    "doublesecretagency/craft-viewcount": "1.0.6",
    "spacecatninja/imager-x": "v3.6.2",
    "hybridinteractive/craft-position-fieldtype": "1.0.17",
    "topshelfcraft/wordsmith": "3.3.2",
    "putyourlightson/craft-blitz": "3.12.8",
    "putyourlightson/craft-blitz-recommendations": "1.3.0",
    "jalendport/craft-lockout": "1.0.1",
    "doublesecretagency/craft-googlemaps": "4.1.5",
    "goat/dashboard-configuration": "^1.0",
    "meetgoat/oakland-email-notifications": "^1.0",
    "spicyweb/craft-neo": "2.13.3",
    "studioespresso/craft-scout": "2.7.2",
    "solspace/craft-freeform": "3.13.15",
    "verbb/icon-picker": "1.1.14",
    "nystudio107/craft-vite": "1.0.30",
    "hillholliday/craft-user-manual": "2.1.2",
    "unionco/calendarize": "1.2.18",
    "statikbe/craft-sentry": "^1.1",
    "sentry/sdk": "^3.1",
    "verbb/buttonbox": "3.1.1",
    "presseddigital/linkit": "3.0.4.1",
    "verbb/tablemaker": "3.0.4",
    "solspace/craft-calendar": "3.3.21",
    "verbb/patrol": "4.0.2",
    "percipioglobal/craft-colour-swatches": "1.7.0",
    "studioespresso/craft-dumper": "2.0.0",
    "doublesecretagency/craft-inventory": "2.1.1",
    "besteadfast/craft-preparse-field": "1.4.1"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "modules\\oaklandlibraryeventsmodule\\": "modules/oaklandlibraryeventsmodule/src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.5"
    },
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "./plugins/dashboardconfiguration"
    },
    {
      "type": "path",
      "url": "./plugins/oaklandemailnotifications"
    }
  ],
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "require-dev": {
    "nystudio107/craft-autocomplete": "^1.11"
  }
}
